{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Price Service",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/price",
      "env": {
        "INFLUXDB_URL": "http://localhost:8086",
        "INFLUXDB_TOKEN": "my-super-secret-auth-token",
